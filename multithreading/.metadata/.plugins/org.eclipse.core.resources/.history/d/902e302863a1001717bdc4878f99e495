package com.dtech.soft.learn;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        
    	//create threads
    	ProcessStatus processStatusVod = new ProcessStatus("vod instance");
    	ProcessStatus processStatusLinear = new ProcessStatus("linear instance");
    	try {

        	processStatusVod.start();
        	processStatusVod.sleep(3);
        	
        	processStatusLinear.start();
        	processStatusLinear.sleep(4);
        	
		} catch (Exception e) {
			// TODO: handle exception
		}
    }
}
